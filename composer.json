{
    "name": "dept-of-scrapyard-robotics/system-fans",
    "description": "ScrapyardIO Framework extension library that enabled PWM and GPIO Control of Fans on Linux",
    "type": "library",
    "version": "0.1.1",
    "keywords": [
        "io",
        "linux",
        "embedded",
        "system fans",
        "gpio",
        "pwm"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Project Saturn Studios, LLC",
            "email": "angel@projectsaturnstudios.com"
        }
    ],
    "require": {
        "php": "^8.3",

        "dept-of-scrapyard-robotics/scrapyard-io-framework": "^0.1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "*",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "ScrapyardIO\\Actuation\\SystemFans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
